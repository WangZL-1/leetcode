class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int maxsubsum=nums[nums.size()-1];
        vector<int> dp=nums;
        for(int i=nums.size()-2;i>=0;i--){
            if (dp[i]+dp[i+1]>dp[i]){
                dp[i]=dp[i]+dp[i+1];
            }
        }
        for(int i=0;i<dp.size();i++){
            if(dp[i]>maxsubsum)
                maxsubsum=dp[i];
        }
        return maxsubsum;


    }
};
